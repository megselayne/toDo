<%- include('../partials/boilerplate') %>

    <div class=container>
        <h1><%= todo.title %></h1>
        <h2>Status: <%= todo.status %></h2>
        <h4>Description: <%= todo.description %></h4>
        <div class='right'>
            <a href='/todos/<%= todo.id %>/edit'>Edit</a>
            <form method="POST" action="/todos/<%= todo.id %>?_method=DELETE">
                <input type="submit" value="Delete">
            </form>
        </div>
    </div>




<%- include('../partials/end') %>